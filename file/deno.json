{
  "name": "@logtape/file",
  "version": "1.0.0",
  "license": "MIT",
  "exports": "./mod.ts",
  "imports": {
    "@types/bun": "npm:@types/bun@^1.2.16",
    "#filesink": "./filesink.jsr.ts",
    "#workersink": "./workersink.deno.ts",
    "#worker_spawn": "./worker_spawn.deno.ts",
    "#worker_factory": "./worker_factory.deno.ts"
  },
  "exclude": [
    "coverage/",
    "npm/",
    ".dnt-import-map.json"
  ],
  "tasks": {
    "build": "pnpm build",
    "test": "deno test --allow-read --allow-write --allow-env=TEMP,TMP,TMPDIR",
    "test:node": {
      "dependencies": [
        "build"
      ],
      "command": "node --experimental-transform-types --test"
    },
    "test:bun": {
      "dependencies": [
        "build"
      ],
      "command": "bun test"
    },
    "test-all": {
      "dependencies": [
        "test",
        "test:node",
        "test:bun"
      ]
    }
  }
}
